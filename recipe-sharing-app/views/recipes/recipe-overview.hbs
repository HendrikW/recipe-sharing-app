<h2>Übersicht über alle Rezepte</h2>


<div class="cuisine">
  {{#each cuisine}}
<button>{{this}}</button>
  {{/each}}
</div>


<div class="container">
<div class="row align-items-start">

  {{#each recipes}}
  <div class="col-4 recipe-card">
<img src="{{this.image}}" >  
  <a href='/rezepte/{{this.id}}'> {{this.title}}</a>

</div>  
  {{/each}}

</div>  
</div>


<a href='/rezepte/neu'> Neues Rezept eingeben</a>
